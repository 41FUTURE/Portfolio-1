<div class="row my-5">
  <div class="col-12">
    <div class="container">
      <div id="projects">
        <h2> Projects</h2>
        <div class="cards">
          <% data.projects.each do |project| %>
            <div class="card-product">
              <%= image_tag project.image, alt: "project image" %>
              <div class="card-product-infos">
                <h2><%= project.name %></h2>
                <p><%= project.description %></p>
              </div>
            </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
